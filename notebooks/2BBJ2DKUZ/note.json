      "text": "%ignite\n\nimport org.quil.server._\nimport org.quil.server.Tasks._\n\nimport util.parsing.json._;\n\ndef format(t: Any): String \u003d  { \n  \t     t match {\n  \t\t\t    case  m : Map[String,Any] \n  \t\t\t              \u003d\u003e new JSONObject(m).toString(this.format);            \n  \t\t\t              \n  \t\t\t    case _ \u003d\u003e JSONFormat defaultFormatter t\n  \t    }\n}\n\nval TaskTemplate \u003d Map(\n        \n        \"Interpreter\" -\u003e \"org.quil.interpreter.QuantLibTemplates.QuantLibXmlTemplateInterpreter\",\n        \"Task\"\t-\u003e \"PriceTrade\",\n    \t\"Template\" -\u003e \"Trade.Swap\",\n    \t\"Repository\" -\u003e \"Template\",\n    \t\"MarketData\" -\u003e Map (  \n    \t\t\"Base\"  -\u003e \"Market\"\n    \t),\n    \t\"TradeData\" -\u003e Map (\n    \t\t\"ID\" -\u003e \"1\",\n    \t\t\"Notional\" -\u003e \"2000.0\",\n    \t\t\"Cpty\" -\u003e \"A\"\n    \t)\n    \t\n    )\n    \n\nval Scenarios \u003d List (\n    \"EUR_Swap_1Y\"-\u003e \"0.01\",\n    \"EUR_Swap_1Y\"-\u003e \"0.02\",\n    \"EUR_Swap_1Y\"-\u003e \"0.03\",\n    \"EUR_Swap_1Y\"-\u003e \"0.04\",\n    \"EUR_Swap_1Y\"-\u003e \"0.05\"\n)\n\n\nScenarios foreach { scenario \u003d\u003e {\n    \n        var scenarioTask \u003d TaskTemplate\n    \n        scenarioTask +\u003d (\"MarketData\" -\u003e \n                            \n                            (TaskTemplate(\"MarketData\").asInstanceOf[Map[String,Any]] + (\"Additional\"-\u003e Map(scenario)))\n                            \n                        )\n        \n        var T \u003d Task.fromString(new JSONObject(scenarioTask).toString(format));\n        TaskRunner.runTask(T);\n        \n    }\n}\n",
